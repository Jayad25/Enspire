{
  "name": "enspire",
  "version": "1.0.0",
  "description": "non native speakers prepping for generla interview questions to improve their communication",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "server:debug": "node --inspect app.js",
    "server": "nodemon app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "keywords": [
    "interview",
    "questions",
    "to",
    "improve",
    "communication"
  ],
  "author": "Safuh, Jaya, and Erica",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-bar-chart": "^0.1.2",
    "react-chartkick": "^0.3.0",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
